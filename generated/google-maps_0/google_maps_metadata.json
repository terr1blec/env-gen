{
  "name": "Google Maps MCP Server",
  "description": "A FastMCP server providing Google Maps-like functionality using offline dataset",
  "version": "1.0.0",
  "tools": [
    {
      "name": "maps_geocode",
      "description": "Convert an address to geographic coordinates",
      "input_schema": {
        "type": "object",
        "properties": {
          "address": {
            "type": "string",
            "description": "The address to geocode"
          }
        },
        "required": ["address"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "results": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "formatted_address": {
                  "type": "string"
                },
                "geometry": {
                  "type": "object",
                  "properties": {
                    "location": {
                      "type": "object",
                      "properties": {
                        "lat": {
                          "type": "number"
                        },
                        "lng": {
                          "type": "number"
                        }
                      }
                    }
                  }
                },
                "place_id": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_reverse_geocode",
      "description": "Convert geographic coordinates to a human-readable address",
      "input_schema": {
        "type": "object",
        "properties": {
          "latitude": {
            "type": "string",
            "description": "The latitude coordinate"
          },
          "longitude": {
            "type": "string",
            "description": "The longitude coordinate"
          }
        },
        "required": ["latitude", "longitude"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "results": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "formatted_address": {
                  "type": "string"
                },
                "geometry": {
                  "type": "object",
                  "properties": {
                    "location": {
                      "type": "object",
                      "properties": {
                        "lat": {
                          "type": "number"
                        },
                        "lng": {
                          "type": "number"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_search_places",
      "description": "Search for places based on a query",
      "input_schema": {
        "type": "object",
        "properties": {
          "query": {
            "type": "string",
            "description": "The search query"
          },
          "radius": {
            "type": "string",
            "description": "Optional search radius in meters"
          },
          "location": {
            "type": "string",
            "description": "Optional location to center the search"
          }
        },
        "required": ["query"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "results": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "name": {
                  "type": "string"
                },
                "formatted_address": {
                  "type": "string"
                },
                "place_id": {
                  "type": "string"
                },
                "types": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "rating": {
                  "type": "number"
                },
                "user_ratings_total": {
                  "type": "integer"
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_place_details",
      "description": "Get detailed information about a specific place",
      "input_schema": {
        "type": "object",
        "properties": {
          "place_id": {
            "type": "string",
            "description": "The unique identifier for the place"
          }
        },
        "required": ["place_id"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "result": {
            "type": "object",
            "properties": {
              "name": {
                "type": "string"
              },
              "formatted_address": {
                "type": "string"
              },
              "formatted_phone_number": {
                "type": "string"
              },
              "website": {
                "type": "string"
              },
              "rating": {
                "type": "number"
              },
              "user_ratings_total": {
                "type": "integer"
              },
              "types": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "geometry": {
                "type": "object",
                "properties": {
                  "location": {
                    "type": "object",
                    "properties": {
                      "lat": {
                        "type": "number"
                      },
                      "lng": {
                        "type": "number"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_distance_matrix",
      "description": "Calculate travel distance and time between multiple origins and destinations",
      "input_schema": {
        "type": "object",
        "properties": {
          "origins": {
            "type": "string",
            "description": "Optional comma-separated list of origin addresses"
          },
          "destinations": {
            "type": "string",
            "description": "Optional comma-separated list of destination addresses"
          }
        }
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "rows": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "elements": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "distance": {
                        "type": "object",
                        "properties": {
                          "text": {
                            "type": "string"
                          },
                          "value": {
                            "type": "integer"
                          }
                        }
                      },
                      "duration": {
                        "type": "object",
                        "properties": {
                          "text": {
                            "type": "string"
                          },
                          "value": {
                            "type": "integer"
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_elevation",
      "description": "Get elevation data for specific locations",
      "input_schema": {
        "type": "object",
        "properties": {
          "locations": {
            "type": "string",
            "description": "Comma-separated list of coordinates (lat,lng) or addresses"
          }
        },
        "required": ["locations"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "results": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "location": {
                  "type": "object",
                  "properties": {
                    "lat": {
                      "type": "number"
                    },
                    "lng": {
                      "type": "number"
                    }
                  }
                },
                "elevation": {
                  "type": "number"
                }
              }
            }
          }
        }
      }
    },
    {
      "name": "maps_directions",
      "description": "Get directions between two locations",
      "input_schema": {
        "type": "object",
        "properties": {
          "origin": {
            "type": "string",
            "description": "The starting location"
          },
          "destination": {
            "type": "string",
            "description": "The ending location"
          },
          "mode": {
            "type": "string",
            "description": "Travel mode",
            "default": "driving"
          }
        },
        "required": ["origin", "destination"]
      },
      "output_schema": {
        "type": "object",
        "properties": {
          "status": {
            "type": "string",
            "description": "Response status"
          },
          "routes": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "summary": {
                  "type": "string"
                },
                "legs": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "distance": {
                        "type": "object",
                        "properties": {
                          "text": {
                            "type": "string"
                          },
                          "value": {
                            "type": "integer"
                          }
                        }
                      },
                      "duration": {
                        "type": "object",
                        "properties": {
                          "text": {
                            "type": "string"
                          },
                          "value": {
                            "type": "integer"
                          }
                        }
                      },
                      "steps": {
                        "type": "array",
                        "items": {
                          "type": "object",
                          "properties": {
                            "instruction": {
                              "type": "string"
                            },
                            "distance": {
                              "type": "object",
                              "properties": {
                                "text": {
                                  "type": "string"
                                },
                                "value": {
                                  "type": "integer"
                                }
                              }
                            },
                            "duration": {
                              "type": "object",
                              "properties": {
                                "text": {
                                  "type": "string"
                                },
                                "value": {
                                  "type": "integer"
                                }
                              }
                            }
                          }
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  ]
}