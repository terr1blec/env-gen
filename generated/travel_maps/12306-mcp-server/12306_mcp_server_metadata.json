{
  "description": "FastMCP server for searching 12306 train tickets using offline database",
  "name": "12306 MCP Server",
  "tools": [
    {
      "description": "查询12306火车票 (Search 12306 train tickets)",
      "input_schema": {
        "properties": {
          "arrival_station": {
            "description": "到达站 (Arrival station)",
            "type": "string"
          },
          "date": {
            "description": "出发日期，格式为 YYYY-MM-DD (Departure date in YYYY-MM-DD format)",
            "type": "string"
          },
          "departure_station": {
            "description": "出发站 (Departure station)",
            "type": "string"
          }
        },
        "required": [
          "departure_station",
          "arrival_station",
          "date"
        ],
        "type": "object"
      },
      "name": "search",
      "output_schema": {
        "properties": {
          "search_parameters": {
            "description": "搜索参数和结果统计 (Search parameters and result statistics)",
            "type": "object"
          },
          "train_tickets": {
            "description": "匹配的火车票列表 (List of matching train tickets)",
            "items": {
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  ]
}